{
  "mcpServers": {
    "mcp-webanalyzer": {
      "command": "C:\\Users\\donghwi\\mcp-webanalyzer\\.venv\\Scripts\\python.exe",
      "args": ["-m", "mcp_webanalyzer.server"],
      "cwd": "C:\\Users\\donghwi\\mcp-webanalyzer",
      "env": {
        "LOG_LEVEL": "INFO",
        "PYTHONPATH": "C:\\Users\\donghwi\\mcp-webanalyzer",
        "REDIS_URL": "redis://localhost:6379/0"
      }
    },
    "mcp-webanalyzer-api": {
      "command": "curl",
      "args": [
        "-X", "POST",
        "-H", "Content-Type: application/json",
        "-H", "Authorization: Bearer YOUR_TOKEN_HERE",
        "http://localhost:8000/mcp/tools/{tool_name}",
        "-d", "{parameters}"
      ],
      "env": {
        "API_BASE_URL": "http://localhost:8000"
      }
    }
  }
}